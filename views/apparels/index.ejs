<%- include('../partials/header') %>

<h2>Apparel List</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Brand</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% apparels.forEach(function(a) { %>
      <tr>
        <td><%= a.name %></td>
        <td><%= a.brand %></td>
        <td><a href="/apparels/<%= a._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    
  </tbody>  
</table>



<%- include('../partials/footer') %>
